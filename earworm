lyrics = ["I wanna be your endgame", "I wanna be your first string",
          "I wanna be your A-Team", "I wanna be your endgame, endgame"]
lines_of_sanity = 6

count = 0
 
while count < lines_of_sanity:        
    for verse in lyrics:
        if count >= lines_of_sanity:
            break
        print(verse)
        count += 1

print("MAKE IT STOP")

