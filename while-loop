#subtracts 1 from all mystery_int until all are less than or equal to 0
mystery_int_1 = 3
mystery_int_2 = 4
mystery_int_3 = 5

while mystery_int_1 > 0 or mystery_int_2 > 0 or mystery_int_3 > 0:
    mystery_int_1 -= 1
    mystery_int_2 -= 1
    mystery_int_3 -= 1
    print(mystery_int_1, mystery_int_2, mystery_int_3)
