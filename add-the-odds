mystery_int = 50

#Add some code below that will find and print the sum of every odd number between 0 and mystery_int.
# Exclude the bounds (e.g. if mystery_int was 51, add the odds from 1 to 49, but not 51).

sum_odds = 0

for i in range(0, mystery_int):
    if i % 2 == 0: #even
        continue
    else: #odd
        sum_odds += i
        #print("adding", i)
        #print(sum_odds)
print("Final sum:", sum_odds)
