listOfNumbers = [91, 92, 93, 94, 95, 96, 97, 98, 99, 100]

sum = 0
for currentNumber in listOfNumbers:
    sum += currentNumber
print(sum / len(listOfNumbers))   

#alternative
sum = 0
for i in range(0, len(listOfNumbers)):
    currentNumber = listOfNumbers[i]
    sum += currentNumber
print(sum / len(listOfNumbers))   

