beats_per_measure = 4
measures = 5

for i in range(1, measures+1):
    for j in range(1, beats_per_measure+1):
        print("Measure: {}, Beat: {}".format(i,j))

# alternative method of counting: 1234 2234 3234 4234 5234
for measure in range(1, measures+1):
    for beat in range(1, beats_per_measure+1):
        if beat == 1:
            print(measure)
        else:
            print(beat)
